<h3 class="text-center">Welcome to <strong>CrossFit Interchange</strong></h3>
<h4 class="text-center">Administration &amp; Content Control Panel</h4>
<center>
<br /><br />
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
        <br /><br />
        <%= f.email_field :email, autofocus: true, placeholder: 'Email', class: 'expanded' %>
        
        <br />
        <%= f.password_field :password, autocomplete: "off", placeholder: 'Password' %>

      <br />
        <% if devise_mapping.rememberable? -%>
          <div class=" text-center">
            <%= f.check_box :remember_me %>
            <%= f.label :remember_me, class: 'subheader' %>
          </div>
        <% end -%>

        <%= render "devise/shared/links" %>

      <hr class="small-7 small-centered columns">
        <center><%= f.submit "Log in" , class: 'hollow button'%></center>
    <% end %>
    </center>
    <!-- Fix center everything with F6 Update -->