<!-- Scaffolding generated by Casein v5.1.1.5 -->

<table class="table table-striped table-hover table-condensed">
	<thead>
		<tr>
			<th><%= casein_sort_link "Desktop", :desktop %></th>
			<th><%= casein_sort_link "Mobile", :mobile %></th>
			<th>&nbsp;</th>
		</tr>
	</thead>
	
	<% cf_schedules.each do |cf_schedule| %>
		<tr>
			<td><%= casein_table_cell_link cf_schedule.desktop, casein_cf_schedule_path(cf_schedule) %></td>
			<td><%= casein_table_cell_link cf_schedule.mobile, casein_cf_schedule_path(cf_schedule) %></td>
			<td class="delete">
				<%= link_to(casein_show_row_icon("trash"), casein_cf_schedule_path(cf_schedule), :method => :delete, :data => { :confirm => "Are you sure you want to delete this cf schedule?" }) %>
			</td>
		</tr>
	<% end %>
</table>