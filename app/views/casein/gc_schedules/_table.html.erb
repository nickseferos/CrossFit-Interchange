<table class="table table-striped table-hover table-condensed">
	<thead>
		<tr>
			<th><%= casein_sort_link "Desktop", :desktop %></th>
			<th><%= casein_sort_link "Mobile", :mobile %></th>
			<th>&nbsp;</th>
		</tr>
	</thead>
	
	<% gc_schedules.each do |gc_schedule| %>
		<tr>
			<td><%= casein_table_cell_link gc_schedule.desktop, casein_gc_schedule_path(gc_schedule) %></td>
			<td><%= casein_table_cell_link gc_schedule.mobile, casein_gc_schedule_path(gc_schedule) %></td>
			<td class="delete">
				<%= link_to(casein_show_row_icon("trash"), casein_gc_schedule_path(gc_schedule), :method => :delete, :data => { :confirm => "Are you sure you want to delete this gc schedule?" }) %>
			</td>
		</tr>
	<% end %>
</table>